{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "handle",
        "name"
    ],
    "properties": {
        "handle": {
            "$id": "#/properties/handle",
            "type": "string",
            "title": "The Handle Schema",
            "default": "",
            "examples": [
                "amzn"
            ],
            "pattern": "^(.*)$"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
                "Amazon"
            ],
            "pattern": "^(.*)$"
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The Num_employees Schema",
            "default": 0,
            "examples": [
                1000
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The Description Schema",
            "default": "",
            "examples": [
                "Ecommerce Company"
            ],
            "pattern": "^(.*)$"
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The Logo_url Schema",
            "default": "",
            "examples": [
                "https://pmcvariety.files.wordpress.com/2018/01/amazon-logo.jpg?w=1000&h=562&crop=1"
            ],
            "pattern": "^(.*)$"
        }
    }
}